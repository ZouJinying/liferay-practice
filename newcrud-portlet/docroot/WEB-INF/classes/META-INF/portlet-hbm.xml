<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.Hotel.model.Hotel" />
	<class name="com.Hotel.model.impl.HotelImpl" table="Hotel">
		<cache usage="read-write" />
		<id name="hotelId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="quantityRooms" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>